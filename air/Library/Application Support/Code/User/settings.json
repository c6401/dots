{
  "cSpell.userWords": [
    "autocorrect",
    "bfloat",
    "committerdate",
    "conflictstyle",
    "contentblocking",
    "CPPFLAGS",
    "cuda",
    "dtype",
    "elif",
    "endfor",
    "endmacro",
    "excludesfile",
    "faiss",
    "finetuning",
    "fromarray",
    "imap",
    "LDFLAGS",
    "localprofile",
    "lxml",
    "numpy",
    "ollama",
    "onnx",
    "openblas",
    "osascript",
    "pathlib",
    "pbpaste",
    "pipx",
    "pretrained",
    "pyaudio",
    "pyenv",
    "readtext",
    "safebrowsing",
    "sessionstore",
    "soundfile",
    "startswith",
    "syncthing",
    "textwrap",
    "tkinter",
    "tldr",
    "tostring",
    "urllib",
    "vectorize",
    "viewset",
    "virtualenv",
    "websockets",
    "xonsh"
  ],
  // "editor.fontLigatures": "'ss09', 'zero', 'onum' 'ss01'",
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  "files.autoSave": "afterDelay",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": false
  },
  "gitlens.ai.experimental.provider": "openai",
  "macros.list": {
    "shellExecute": [
      "expandLineSelection",
      "workbench.action.terminal.runSelectedText",
      "cancelSelection"
    ]
  },
  "notebook.lineNumbers": "on",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.middleClickBehavior": "paste",
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape",
    },
    "jj": {
      "command": "vscode-neovim.escape",
    }
  },
  "git.confirmSync": false,
  "editor.renderWhitespace": "all",
  "jupyter.experiments.enabled": false,
  "jupyter.interactiveWindow.cellMarker.decorateCells": "allCells",
  "mcp": {
    "servers": {
      "mcp-server-time": {
        "command": "uvx",
        "args": [
          "--with",
          "mcp_server_time",
          "python",
          "-m",
          "mcp_server_time",
          "--local-timezone=America/Los_Angeles"
        ],
        "env": {}
      },
      "playwright": {
        "command": "npx",
        "args": [
          "@playwright/mcp@latest"
        ]
      },
      "mcp-run-python": {
        "command": "deno",
        "args": [
            "run",
            "-N",
            "-R=node_modules",
            "-W=node_modules",
            "--node-modules-dir=auto",
            "jsr:@pydantic/mcp-run-python",
            "stdio",
        ],
        "env": {}
      }
    }
  },
  "chat.mcp.discovery.enabled": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  // "python.analysis.typeCheckingMode": "strict",
}
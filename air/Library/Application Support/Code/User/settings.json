{
  "cSpell.userWords": [
    "autocorrect",
    "bfloat",
    "committerdate",
    "conflictstyle",
    "contentblocking",
    "CPPFLAGS",
    "cuda",
    "dtype",
    "elif",
    "endfor",
    "endmacro",
    "excludesfile",
    "faiss",
    "finetuning",
    "fromarray",
    "imap",
    "LDFLAGS",
    "localprofile",
    "lxml",
    "numpy",
    "ollama",
    "onnx",
    "openblas",
    "osascript",
    "pathlib",
    "pbpaste",
    "pipx",
    "pretrained",
    "pyaudio",
    "pyenv",
    "readtext",
    "safebrowsing",
    "sessionstore",
    "soundfile",
    "startswith",
    "syncthing",
    "textwrap",
    "tkinter",
    "tldr",
    "tostring",
    "urllib",
    "vectorize",
    "viewset",
    "virtualenv",
    "websockets",
    "xonsh"
  ],
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  "files.autoSave": "afterDelay",
  "gitlens.ai.experimental.provider": "openai",
  "macros.list": {
    "shellExecute": [
      "expandLineSelection",
      "workbench.action.terminal.runSelectedText",
      "cancelSelection"
    ]
  },
  "notebook.lineNumbers": "on",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.middleClickBehavior": "paste",
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape",
    },
    "jj": {
      "command": "vscode-neovim.escape",
    }
  },
  "git.confirmSync": false,
  "editor.renderWhitespace": "all",
  "jupyter.experiments.enabled": false,
  "jupyter.interactiveWindow.cellMarker.decorateCells": "allCells",
  "chat.mcp.discovery.enabled": {
    "claude-desktop": true,
    "windsurf": true,
    "cursor-global": true,
    "cursor-workspace": true
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "watch\\.*",
        "cmd": "bin/onsave${fileExtname}.sh '${file}' || true",
        "messageAfter": "- 1. Complete",
      },
      {
        "match": ".*\\.watch\\.*",
        "cmd": "bin/onsave${fileExtname}.sh '${file}' || true",
        "messageAfter": "- 1. Complete",
      },
    ]
  },
  // "python.analysis.typeCheckingMode": "strict",
  // "editor.fontLigatures": "'ss09', 'zero', 'onum' 'ss01'",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "docker.extension.enableComposeLanguageServer": false,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "terminal.integrated.suggest.enabled": true,
}